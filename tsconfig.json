{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "es6",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "strictNullChecks": true,
    "typeRoots": [ "node_modules/types" ],
    "types": [ "jquery" ],
    "lib":[ "es2015", "dom" ],
    "plugins": [
      { "name": "typescript-tslint-plugin" }
    ]
  }
}
